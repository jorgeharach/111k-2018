<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="campos.model.EstadoCampo" table="estado_campo">
        <id name="id" column="esc_id">
            <generator class="increment"/>
        </id>
        <property name="nombre" column="esc_nombre" />
        <property name="descripcion" column="esc_descripcion" />
    </class>
</hibernate-mapping>
